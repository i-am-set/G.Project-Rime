[sub_resource type="Shader" id="Shader_ycexp"]
code = "shader_type canvas_item;

uniform vec4 head_map : source_color;
uniform vec4 head_color : source_color;
uniform vec4 torso_map : source_color;
uniform vec4 torso_color : source_color;
uniform vec4 left_arm_map : source_color;
uniform vec4 left_arm_color : source_color;
uniform vec4 right_arm_map : source_color;
uniform vec4 right_arm_color : source_color;
uniform vec4 left_leg_map : source_color;
uniform vec4 left_leg_color : source_color;
uniform vec4 right_leg_map : source_color;
uniform vec4 right_leg_color : source_color;

void fragment() {
    vec4 pixel_color = texture(TEXTURE, UV);
    vec3 rgb_color = pixel_color.rgb; // Extract the RGB values

    if (pixel_color.a > 0.1) { // Check if alpha is above 0.1
        if (rgb_color == left_leg_map.rgb) { // red
            COLOR = left_leg_color;
        } else if (rgb_color == right_leg_map.rgb) { // green
            COLOR = right_leg_color;
        } else if (rgb_color == right_arm_map.rgb) { // blue
            COLOR = right_arm_color;
        } else if (rgb_color == left_arm_map.rgb) { // yellow
            COLOR = left_arm_color;
        } else if (rgb_color == torso_map.rgb) { // cyan
            COLOR = torso_color;
        } else if (rgb_color == head_map.rgb) { // pink
            COLOR = head_color;
        } else {
            COLOR = pixel_color;
        }
    } else {
        COLOR = pixel_color;
    }
}"

