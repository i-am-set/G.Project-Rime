[sub_resource type="Shader" id="Shader_522v6"]
code = "shader_type canvas_item;

uniform vec4 background_color : source_color;

void fragment() {
    vec4 texColor = texture(TEXTURE, UV);
    float gray = dot(texColor.rgb, vec3(0.299, 0.587, 0.114));
    texColor.a = 1.0 - gray;
    texColor.rgb = mix(texColor.rgb, background_color.rgb, background_color.a * (1.0 - gray));
    COLOR = texColor;
}"

